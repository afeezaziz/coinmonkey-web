  "liveNow": false,
  "panels": [
    {
      "type": "timeseries",
      "title": "HTTP Requests (rps) by Endpoint/Status",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "options": {"legend": {"displayMode": "table", "placement": "bottom"}},
      "targets": [
        {
          "expr": "sum by (endpoint, status) (rate(cm_http_requests_total[5m]))",
          "legendFormat": "{{endpoint}} {{status}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "HTTP Latency P95 by Endpoint (s)",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "options": {"legend": {"displayMode": "table", "placement": "bottom"}},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, endpoint) (rate(cm_http_request_latency_seconds_bucket[5m])))",
          "legendFormat": "{{endpoint}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Scheduler Ticks & Errors (rps)",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "options": {"legend": {"displayMode": "table", "placement": "bottom"}},
      "targets": [
        {
          "expr": "rate(cm_scheduler_tick_total[1m])",
          "legendFormat": "ticks",
          "refId": "A"
        },
        {
          "expr": "rate(cm_scheduler_errors_total[1m])",
          "legendFormat": "errors",
          "refId": "B"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Alert Webhook Duration P95 (s)",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "options": {"legend": {"displayMode": "table", "placement": "bottom"}},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(cm_alert_webhook_duration_seconds_bucket[5m]))",
          "legendFormat": "duration p95",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Alert Webhook Attempts (rps) by Result",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
      "options": {"legend": {"displayMode": "table", "placement": "bottom"}},
      "targets": [
        {"expr": "rate(cm_alert_webhook_attempts_total{result=\"success\"}[5m])", "legendFormat": "success", "refId": "A"},
        {"expr": "rate(cm_alert_webhook_attempts_total{result=\"error\"}[5m])", "legendFormat": "error", "refId": "B"},
        {"expr": "rate(cm_alert_webhook_attempts_total{result=\"blocked\"}[5m])", "legendFormat": "blocked", "refId": "C"}
      ]
    },
    {
      "type": "timeseries",
      "title": "Orchestrator Actions (rps)",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
      "options": {"legend": {"displayMode": "table", "placement": "bottom"}},
      "targets": [
        {"expr": "sum by (action, result) (rate(cm_orchestrator_actions_total[5m]))", "legendFormat": "{{action}} {{result}}", "refId": "A"}
      ]
    },
    {
      "type": "timeseries",
      "title": "Orchestrator Status Checks (rps)",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
      "options": {"legend": {"displayMode": "table", "placement": "bottom"}},
      "targets": [
        {"expr": "rate(cm_orchestrator_status_checks_total{result=\"success\"}[5m])", "legendFormat": "success", "refId": "A"},
        {"expr": "rate(cm_orchestrator_status_checks_total{result=\"error\"}[5m])", "legendFormat": "error", "refId": "B"}
      ]
    },
    {
      "type": "stat",
      "title": "App Health",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"], "fields": "", "values": false},
        "orientation": "horizontal"
      },
      "targets": [
        {"expr": "cm_app_live", "legendFormat": "live", "refId": "A"},
        {"expr": "cm_app_ready", "legendFormat": "ready", "refId": "B"}
      ]
    }
  ],
